package set;

import java.util.HashSet;
import java.util.Set;

public class exemploHashset {

	public static void main(String[] args) {
		
		
		Set<Double> notasAlunos = new HashSet<>();
		
		notasAlunos.add(5.8);
		notasAlunos.add(9.3);
		notasAlunos.add(6.5);
		notasAlunos.add(10d);
		notasAlunos.add(5.4);
		notasAlunos.add(7.3);
		notasAlunos.add(3.8);
		notasAlunos.add(4.0);
		
		System.out.println(notasAlunos);
		
		System.out.println(notasAlunos.remove(3.8));
		
		
	}
	
}
